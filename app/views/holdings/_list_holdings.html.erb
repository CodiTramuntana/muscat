<% if @holdings && @holdings.count > 0%>
<div>
	<div>
		<table border="0" cellspacing="0" cellpadding="0" class="index_table index">
			<tr>
				<th>ID</th>
				<th><%=I18n.t(:siglum)%></th>
			</tr>
		<% @holdings.each do |h| %>
			<tr class="<%= cycle('even', 'odd') -%>">
				<td class="col"><%= h.id %></td>
				<td class="col"><%= h.lib_siglum %></td>
			</tr>
		<% end %>
		</table>
	</div>
</div>
<%end%>