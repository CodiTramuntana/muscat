<dt id="<%= render_panel -%>_tag_dt_<%= tag_name -%>-<%= iterator -%>" class="tag_<%= tag_name -%> pe_grid_panel ui-corner-all">
	<%= render :partial => @editor_profile.get_tag_partial("tag_header", tag_name), 
		:locals => { :render_panel => render_panel, :group => group, :tag_name => tag_name, :marc_tag => marc_tag, :iterator => iterator  } %>
	<div class="pe_grid_tag_tab ui-corner-tl"><span style="color: green"><%= tag_name %></span><span style="color: #444"><strong><%= @editor_profile.get_label(tag_name) -%></strong></span></div>
	<div id="<%= render_panel -%>_tag_<%= tag_name -%>-<%= iterator -%>">
		<%= render :partial => @editor_profile.get_tag_partial("tag", tag_name), 
			:locals => { :render_panel => render_panel, :group => group, :tag_name => tag_name, :marc_tag => marc_tag, :iterator => iterator  } %>
		<%= render :partial => @editor_profile.get_tag_partial("tag_footer", tag_name), 
			:locals => { :render_panel => render_panel, :group => group, :tag_name => tag_name, :marc_tag => marc_tag, :iterator => iterator  } %>
	</div>
</dt>