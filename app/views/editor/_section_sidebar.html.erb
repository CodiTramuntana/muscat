<ul>
<%- @editor_profile.layout_config[:marc_editor_group_links].each do |gl| -%>
    <li><a data-scroll-target="<%= gl.gsub(/ /, "_") -%>" data-panel="marc_editor_panel" href="#"><%= gl -%></a></li>
<%- end -%>
<%- if !@editor_profile.layout_config[:marc_editor_missing_group_links].empty? -%>
    <li>&nbsp;</li>
    <%- missing_group_link = @editor_profile.layout_config[:marc_editor_missing_group_links] %>
    <li><a data-scroll-target="<%= missing_group_link.gsub(/ /, "_") -%>" data-panel="marc_editor_panel" href="#"><%= missing_group_link -%></a></li>
<%- end -%>
	<li class="marc_separator"></li>
	<li class="marc_icon"><a data-scroll-target="show_toplevel" data-panel="marc_editor_panel" href="#" class="marc_icon marc_icon_all"><%=(I18n.t :show_all_groups)%></a></li>
	<li class="marc_icon"><a data-scroll-target="show_preview" data-panel="marc_editor_preview" href="#" class="marc_icon marc_icon_preview"><%=(I18n.t :show_preview)%></a></li>
	<li class="marc_icon"><a data-scroll-target="show_toplevel" data-panel="marc_editor_history" href="#" class="marc_icon marc_icon_save"><%=(I18n.t :save_and_continue)%></a></li>
	<li class="marc_icon"><a data-scroll-target="show_toplevel" data-panel="marc_editor_history" href="#" class="marc_icon marc_icon_history"><%=(I18n.t :show_history)%></a></li>
	<li class="marc_separator"></li>
	<li class="marc_icon"><a data-scroll-target="show_toplevel" data-panel="marc_editor_history" href="#" class="marc_icon marc_icon_cancel"><%=(I18n.t :cancel)%></a></li>
	<li class="marc_icon"><a data-scroll-target="show_toplevel" data-panel="marc_editor_history" href="#" class="marc_icon marc_icon_save_exit"><%=(I18n.t :save_and_exit)%></a></li>
</ul>
